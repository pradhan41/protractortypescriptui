{
    "printWidth": 120,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "overrides": [
        { "files": "*.json", "options": { "tabWidth": 2 } },
        { "files": ".*rc", "options": { "parser": "json" } }
    ]
}
